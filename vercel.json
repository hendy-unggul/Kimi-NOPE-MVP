{
  "rewrites": [
    {
      "source": "/",
      "destination": "/login.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/rage",
      "destination": "/rage.html"
    },
    {
      "source": "/wall",
      "destination": "/wall.html"
    },
    {
      "source": "/(.*)",
      "destination": "/login.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}

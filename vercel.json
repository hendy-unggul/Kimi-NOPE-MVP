{
  "rewrites": [
    {
      "source": "/",
      "destination": "/login.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/onboarding",
      "destination": "/onboarding.html"
    },
    {
      "source": "/home",
      "destination": "/home.html"
    },
    {
      "source": "/(.*)",
      "destination": "/login.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}
